{
    "name": "iviu96afa/yii2-admin",
    "description": "Enhanced auth manager for Yii2",
    "keywords": ["yii2", "admin", "auth"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/iviu96afa/yii2-admin/issues",
        "source": "https://github.com/iviu96afa/yii2-admin"
    },
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-jui": "~2.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
    },
    "autoload": {
        "psr-4": {
            "iviu96afa\\admin\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
